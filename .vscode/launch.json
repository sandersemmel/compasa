{
    "configurations": [
    
        {
            "name": "Launch Program",
            "program": "${workspaceFolder}/back/backend.js",
            "runtimeExecutable": "${workspaceFolder}/node_modules/nodemon/bin/nodemon.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node",
            "console": "integratedTerminal"
        },

        {
            "name": "Node.js - Debug Current File",
            "type": "node",
            "request": "launch",
            "program": "${file}"
          }

    ]
}